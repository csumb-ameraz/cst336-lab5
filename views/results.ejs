<% include partials/header.ejs %>


<form action="/search">
      <input type="text" value="<%=keyword%>" id="keyword" placeholder="Type keyword" name="keyword" /><br /><br />
      <button>
        Search
      </button>
  <a href="/displayKeywords" id="favLink"> See Favorites </a>
    </form>
<% imageURLs.forEach(function(imageURL, i){ %>
<%- (i%3==0)?'<br />':'' %>
<div class="imageContainer">
  <img src='<%=imageURL%>' class="image" width='200' height='200' />
  <img class='favoriteIcon' src='img/favorite.png' width='20'>
</div>
  
<% }); %>
<% include partials/footer.ejs %>